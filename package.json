{
  "devDependencies": {
    "cspell": "^9.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "3.6.2"
  },
  "scripts": {
    "cspell": "cspell '**'",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "cspell",
    "**/*": "prettier --check --ignore-unknown"
  }
}
