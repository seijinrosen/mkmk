{
  "devDependencies": {
    "cspell": "^8.19.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "3.5.3"
  },
  "scripts": {
    "cspell": "cspell '**'",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "cspell",
    "**/*": "prettier --check --ignore-unknown"
  }
}
